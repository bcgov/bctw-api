{
  "name": "bctw-api",
  "version": "1.0.0",
  "description": "Collecting and dispensing BC Telemetry data",
  "main": "start.js",
  "scripts": {
    "dev": "bash -c 'node_modules/supervisor/lib/cli-wrapper.js -w build/server.js build/server.js'",
    "start": "npx pm2@4.4.0 --no-daemon start build/server.js",
    "build": "rimraf ./build && tsc",
    "test": "jest --setupFiles=dotenv/config",
    "test:watch": "jest --setupFiles=dotenv/config --watchAll --detectOpenHandles",
    "test:coverage": "jest --setupFiles=dotenv/config --watchAll --coverage --detectOpenHandles",
    "lint": "eslint . --ext .ts --no-error-on-unmatched-pattern",
    "lint:fix": "eslint . --fix --ignore-pattern 'node_modules' --ext .ts",
    "kill": "npx kill-port 3000 & pkill -f nodemon",
    "start:dev": "nodemon -r dotenv/config ./src/server.ts",
    "start:reload": "./node_modules/.bin/nodemon -r dotenv/config src/server.ts --exec ts-node",
    "watch": "tsc -w",
    "dead:code": "ts-prune | (! grep -v 'used in module')"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/bctw-api/bctw-api"
  },
  "author": "BC Government",
  "license": "ISC",
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/proj4": "^2.5.2",
    "@types/supertest": "^2.0.12",
    "adm-zip": "^0.5.10",
    "axios": "^0.21.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "express": "^4.21.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "needle": "^2.5.0",
    "node-xlsx": "^0.21.0",
    "pg": "^8.2.1",
    "proj4": "^2.9.0",
    "sql-template-strings": "^2.2.2",
    "uuid": "^9.0.0",
    "xml2js": "^0.4.23",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.1.5",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.5",
    "@types/node-xlsx": "^0.15.3",
    "@types/pg": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^7.1.0",
    "jest": "^27.0.4",
    "kill-port": "^2.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "supervisor": "^0.12.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.3.2"
  }
}
